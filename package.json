  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iliazeus/zmbx2gltf/issues"
  },
  "homepage": "https://github.com/iliazeus/zmbx2gltf#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/pngjs": "^6.0.1",
    "@types/source-map-support": "^0.5.6",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "esbuild": "^0.17.11",
    "events": "^3.3.0",
    "prettier": "^2.8.2",
    "process": "^0.11.10",
    "source-map-support": "^0.5.21",
