{
  "name": "zmbx2gltf",
  "version": "0.1.4",
  "description": "A tool and a library to convert MecaBricks .zmbx models to GlTF",
  "keywords": [
    "zmbx",
    "gltf",
    "lego",
    "mecabricks"
  ],
  "main": "./dist/index.js",
  "bin": "./dist/main.js",
  "scripts": {
    "watch": "tsc --watch",
    "start": "node ./dist/main.js",
    "build": "tsc",
    "bundle": "tsc && browserify ./dist/index.js -o ./zmbx2gltf.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iliazeus/zmbx2gltf.git"
  },
  "author": "Ilia Pozdnyakov <ilia.pozdnyakov@ya.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iliazeus/zmbx2gltf/issues"
  },
  "homepage": "https://github.com/iliazeus/zmbx2gltf#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/source-map-support": "^0.5.6",
    "@types/uuid": "^9.0.0",
    "browserify": "^17.0.0",
    "prettier": "^2.8.2",
    "source-map-support": "^0.5.21",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "optionalDependencies": {
    "jimp": "^0.16.2",
    "jszip": "^3.10.1"
  }
}
